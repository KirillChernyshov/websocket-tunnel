{
  "name": "websocket-tunnel",
  "script": "dist/server/index.js",
  "instances": 1,
  "exec_mode": "fork",
  "watch": false,
  "max_memory_restart": "1G",
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000",
    "WS_PORT": "3001"
  },
  "error_file": "./logs/server-error.log",
  "out_file": "./logs/server-out.log",
  "log_file": "./logs/server-combined.log",
  "time": true,
  "restart_delay": 4000,
  "max_restarts": 10,
  "min_uptime": "10s"
}
